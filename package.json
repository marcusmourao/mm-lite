{
  "name": "mm-lite",
  "version": "0.0.6",
  "scripts": {
    "dev": "HOST=0.0.0.0 nuxt",
    "build": "nuxt build",
    "start": "HOST=0.0.0.0 nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "ci:release": "./scripts/release.sh",
    "release": "release-it --ci",
    "release:preview": "release-it --ci --dry-run"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dinero.js": "^1.8.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.6",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^9.1.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@vue/test-utils": "^1.1.0",
    "auto-changelog": "^2.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-nuxt": "^1.0.0",
    "full-icu": "^1.3.1",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "postcss": "^7.0.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^4.0.0",
    "release-it": "^13.6.8",
    "vue-jest": "^3.0.4"
  }
}
